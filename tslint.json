{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "import-name": false,
    "no-var-requires": false,
    "ter-arrow-parens": false,
    "variable-name": false,
    "no-increment-decrement": false,
    "max-line-length": [
      true,
      120
    ],
    "align": false
  }
}
